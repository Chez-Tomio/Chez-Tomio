{
    "extends": "../../tsconfig.base.json",
    "compilerOptions": {
        "target": "es5",
        "lib": ["dom", "dom.iterable", "esnext"],
        "allowJs": true,
        "skipLibCheck": true,
        "noEmit": true,
        "resolveJsonModule": true,
        "isolatedModules": true,
        "jsx": "preserve",
        "strict": false
    },
    "include": ["next-env.d.ts", "images.d.ts", "**/*.ts", "**/*.tsx"],
    "exclude": ["node_modules"]
}
