        "eslint-plugin-react": "^7.23.1",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-scripts": "4.0.2",
        "typescript": "^4.1.3"
    },
    "peerDependencies": {
        "react": "^17.0.2",
        "react-dom": "^17.0.2"
    },
    "dependencies": {
        "@emotion/react": "^11.4.1",
        "react-device-detect": "^1.17.0",
